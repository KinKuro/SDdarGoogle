<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="300dp"
    android:layout_height="200dp"
    android:layout_gravity="center"
    android:padding="32dp"
    android:background="#888888">

    <TextView
        android:id="@+id/tv_start_dialog_map"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:text="출발지점"
        android:gravity="center"
        android:background="@drawable/selector_back_tv_flag1"
        android:layout_above="@id/tv_destination_dialog_map"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="16dp"
        android:clickable="true"/>

    <TextView
        android:id="@+id/tv_destination_dialog_map"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:text="도착지점"
        android:gravity="center"
        android:background="@drawable/selector_back_tv_flag1"
        android:layout_centerInParent="true"
        android:clickable="true"/>

    <ImageView
        android:id="@+id/btn_ok_dialog_map"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/selector_btn_submit"
        android:layout_alignParentBottom="true"
        android:clickable="true"/>

    <ImageView
        android:id="@+id/btn_cancel_dialog_map"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/selector_btn_cancel"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:clickable="true"/>



</RelativeLayout>